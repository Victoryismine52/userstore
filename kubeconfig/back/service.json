{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "namespace":"userstore",
    "name": "api-userstore"
  },
  "spec": {
    "selector": {
      "app": "api-userstore"
    },
    "ports": [
      {
        "protocol": "TCP",
        "port": 80,
        "targetPort": 9000,
        "nodePort": 32495
      }
    ],
    "type": "LoadBalancer"
  }
}